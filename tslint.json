{
  "extends": [
    "tslint-config-airbnb"
  ],
  "rules": {
    "variable-name": [
      true,
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ],
    "linebreak-style": [
      true,
      "LF"
    ],
    "arrow-parens": [
      "error",
      "as-needed"
    ]
  },
  "rulesDirectory": []
}